{
	"compilerOptions": {
		"noEmit": true,
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"strict": true,
		"alwaysStrict": true,
		"jsx": "react",
		"jsxFactory": "React.createElement",
		"jsxFragmentFactory": "React.Fragment",
		"types": ["react/next", "react-dom/next"]
	}
}
